{
    "schema": {
        "type": "object",
        "title": "Settings",
        "properties": {
            "smooth": {
                "title": "Smooth out",
                "type": "boolean",
                "default": false
            },
            "showlegend": {
                "title": "Show Legend",
                "type": "boolean",
                "default": false
            },

            "button": {
                "title": "Button settings",
                "type": "object",
                "properties": {
                    "legendcolortext": {
                        "title": "Legend Color",
                        "type": "string",
                        "default": "#000000"
                    }
                }
            },

            "yAxisLeftTitle": {
                "title": "Title Left Y-Axis",
                "type": "string",
                "default": ""
            },
            "yAxisLeftUnit": {
                "title": "Unit Left Y-Axis",
                "type": "string",
                "default": "ml"
            },
            "yAxisRightTitle": {
                "title": "Title Right Y-Axis",
                "type": "string",
                "default": ""
            },
            "yAxisRightUnit": {
                "title": "Unit Right Y-Axis",
                "type": "string",
                "default": ""
            },
            "showDataPoints": {
                "title": "Show Data points",
                "type": "boolean",
                "default": false
            },
            "movingseriesMarkLine": {
                "title": "Max, Min, Avg percent for moving",
                "type": "number",
                "default": 0
            },
            "symbolSize_data": {
                "title": "Size of Data points",
                "type": "number",
                "default": 5
            },
            "showPanZoomTool": {
                "title": "Show Pan and Zoom tool",
                "type": "boolean",
                "default": true
            },

            "yAxisLeftAutoScale": {
                "title": "Left Y-Axis Autoscale",
                "type": "boolean",
                "default": true
            },
            "yAxisLeftMinScale": {
                "title": "Left Y-Axis Min-Value",
                "type": "number",
                "default": 0
            },
            "yAxisLeftMaxScale": {
                "title": "Left Y-Axis Max-Value",
                "type": "number",
                "default": 100
            },
            "yAxisRightAutoScale": {
                "title": "Right Y-Axis Autoscale",
                "type": "boolean",
                "default": true
            },
            "yAxisRightMinScale": {
                "title": "Right Y-Axis Min-Value",
                "type": "number",
                "default": 0
            },
            "yAxisRightMaxScale": {
                "title": "Right Y-Axis Max-Value",
                "type": "number",
                "default": 100
            },
            "yAxisRightColor": {
                "title": "Right y-Axis color",
                "type": "object",
                "properties": {
                    "yAxisRightColorChoser": {
                        "title": "Right y-Axis Color",
                        "type": "string",
                        "default": "#F44336"
                    }
                }
            },
            "grid_layout_right": {
                "title": "Chart to the left",
                "type": "number",
                "default": 40
            },
            "grid_layout_top": {
                "title": "Chart to the top",
                "type": "number",
                "default": 0
            },
            "grid_layout_bottom": {
                "title": "Chart to the bottom",
                "type": "number",
                "default": 0
            },
            "grid_layout_left": {
                "title": "Chart to the right",
                "type": "number",
                "default": 130
            },
            "markline_layout_left_or_right": {
                "title": "Markline Left or Right",
                "type": "number",
                "default": 285
            },
            "annotations": {
                "title": "Annotations",
                "type": "array",
                "minItems": 0,
                "items": {
                    "title": "Attribute",
                    "type": "object",
                    "properties": {
                        "annotationsType": {
                            "title": "Annotations Type",
                            "type": "string",
                            "enum": [
                                "Fixed",
                                "Attribute"
                            ],
                            "default": "Fixed"
                        },
                        "fixedNumber": {
                            "title": "Fixed Number",
                            "type": "number",
                            "default": 10
                        },
                        "attribute": {
                            "title": "Attribute",
                            "type": "string"
                        },
                        "description": {
                            "title": "Description",
                            "type": "string"
                        },
                        "color": {
                            "title": "Color",
                            "type": "string",
                            "enum": [
                                "Red",
                                "Green",
                                "Blue",
                                "Yellow"
                            ],
                            "default": "Green"
                        },

                        "attributeType": {
                            "title": "Attribute Type",
                            "type": "string",
                            "enum": [
                                "Client",
                                "Server"
                            ],
                            "default": "Client"
                        },
                        "axisAssignment": {
                            "title": "Axis Assignment",
                            "type": "string",
                            "enum": [
                                "Left",
                                "Right"
                            ],
                            "default": "Left"
                        }
                    },
                    "required": ["color"]
                }
            },
            "debugOutput": {
                "title": "Use Debbug Output",
                "type": "boolean",
                "default": false
            }

        }
    },
    "form": [
        [
            "showlegend",

            {
                "key": "button",
                "items": [{
                    "key": "button.legendcolortext",
                    "type": "color"
                }]
            }

        ],
        [
            "smooth",
            "yAxisLeftTitle",
            "yAxisLeftUnit",
            "yAxisLeftAutoScale",
            "yAxisLeftMinScale",
            "yAxisLeftMaxScale",
            "yAxisRightTitle",
            "yAxisRightUnit",
            "yAxisRightAutoScale",
            "yAxisRightMinScale",
            "yAxisRightMaxScale",
            {
                "key": "yAxisRightColor",
                "items": [{
                    "key": "yAxisRightColor.yAxisRightColorChoser",
                    "type": "color"
                }]
            },
            "showDataPoints",
            "symbolSize_data",
            "showPanZoomTool",
            "annotations",
            "markline_layout_left_or_right",
            "grid_layout_right",
            "grid_layout_left",
            "grid_layout_top",
            "grid_layout_bottom"

        ],
        [
            "debugOutput"
        ]

    ],
    "groupInfoes": [{
            "formIndex": 0,
            "GroupTitle": "Legend Style Settings"
        },
        {
            "formIndex": 1,
            "GroupTitle": "Graph Settings"
        },
        {
            "formIndex": 2,
            "GroupTitle": "Debug"
        }
    ]
}